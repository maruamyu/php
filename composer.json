{
    "name": "maruamyu/web-app-lib",
    "type": "library",
    "description": "simple and useful library for web application.",
    "keywords": ["psr-7", "http", "request", "response", "oauth", "oauth2", "openid", "jwt", "ulid"],
    "homepage": "https://github.com/maruamyu/php-lib",
    "license": "MIT",
    "authors": [
        {
            "name": "maruamyu.net project",
            "homepage": "http://maruamyu.net/",
            "role": "Manager"
        },
        {
            "name": "mirai-iro",
            "homepage": "http://xn--r9j5bxu.jp/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "psr/http-message": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*"
    },
    "suggest": {
        "ext-mcrypt": "if use Rijndael (`Maruamyu\\Core\\Cipher\\Aes`)",
        "ext-bcmath": "if used 32bit PHP (`Maruamyu\\Core\\Ulid`)"
    },
    "autoload": {
        "psr-4": {
            "Maruamyu\\Core\\": "src/Maruamyu/Core/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v2.3.x-dev"
        }
    }
}
